<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>FacebookController</servlet-name>
  	<display-name>FacebookController</display-name>
  	<description></description>
  	<servlet-class>com.facebook.controller.FacebookController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>FacebookLoginController</servlet-name>
  	<display-name>FacebookLoginController</display-name>
  	<description></description>
  	<servlet-class>com.facebook.controller.FacebookLoginController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>FacebookController</servlet-name>
  	<url-pattern>/FacebookController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>FacebookLoginController</servlet-name>
  	<url-pattern>/FacebookLoginController</url-pattern>
  </servlet-mapping>
</web-app>
